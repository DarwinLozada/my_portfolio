{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended", "prettier" , "standard"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "trailingComma": "all"
      }
    ],
    "comma-dangle": ["error", "always-multiline"],
    "space-before-function-paren": ["error", {
      "anonymous": "never",
      "named": "never",
      "asyncArrow": "always"
    }],
    "multiline-ternary": ["error", "never"]
  }
}
